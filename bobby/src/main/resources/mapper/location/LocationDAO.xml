<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--这里的namespace就是指的你要实现的dao层接口的类路径 -->
<mapper namespace="com.bobby.dao.location.LocationDAO">

	<insert id="saveLocation" parameterType="com.bobby.dto.common.LocationDTO">
		insert into t_location_data(ID,NICK_NAME,LATITUDE,LONGITUDE,SPEED,ALTITUDE,REPORT_TIME) 
		values(#{id},#{nickName},#{latitude},#{longitude},#{speed},#{altitude},#{reportTime})
	</insert>
</mapper>